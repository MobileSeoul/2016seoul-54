<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kkyubrother.busta_chat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="BUSTA"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DPushActivity"/>
        <activity android:name=".DPushLogin"/>
        <activity android:name=".DPushProdKeyCheck"/>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".ReviewActivity"></activity>
        <activity android:name=".ReviewMakeActivity"></activity>
        <activity android:name=".ReviewViewActivity"></activity>

    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
</manifest>